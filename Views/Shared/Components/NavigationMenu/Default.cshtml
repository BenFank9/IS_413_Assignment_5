@model IEnumerable<string>

<a class="btn btn-block btn-outline-secondary" asp-action="Index" asp-controller="Home" asp-route-category="">Home</a>

@foreach (string category in Model)
    
{
@*highlight this or don't highlight this.*@

    <a  class="btn @(category == ViewBag.SelectedCategory ? "btn-primary" : "btn-outline-secondary") mya" asp-action="Index" asp-controller="Home" asp-route-category="@category" asp-route-page="1">@category</a>
}